#: gstreamer/backend.cpp:220
msgctxt "Phonon::Gstreamer::Backend|"
msgid ""
"Warning: You do not seem to have the package gstreamer1.0-plugins-good "
"installed.\n"
"          Some video features have been disabled."
msgstr ""
"Peringatan: Sepertinya kamu belum memiliki paket gstreamer1.0-plugins-good "
"yang terinstal.\n"
"          Beberapa fitur video akan dinonfungsikan."

#: gstreamer/backend.cpp:228
msgctxt "Phonon::Gstreamer::Backend|"
msgid ""
"Warning: You do not seem to have the base GStreamer plugins installed.\n"
"          All audio and video support has been disabled"
msgstr ""
"Peringatan: Sepertinya kamu belum memiliki dasar plugin GStreamer yang "
"terinstal.\n"
"          Semua dukungan audio dan video akan dinonfungsikan"

#: gstreamer/mediaobject.cpp:428
msgctxt "Phonon::Gstreamer::MediaObject|"
msgid "Default"
msgstr "Baku"

#: gstreamer/mediaobject.cpp:442 gstreamer/mediaobject.cpp:475
msgctxt "Phonon::Gstreamer::MediaObject|"
msgid "Unknown"
msgstr "Tak diketahui"

#: gstreamer/mediaobject.cpp:462
msgctxt "Phonon::Gstreamer::MediaObject|"
msgid "Disable"
msgstr "Nonfungsikan"

#: gstreamer/pipeline.cpp:515
msgctxt "Phonon::Gstreamer::Pipeline|"
msgid "One or more plugins are missing in your GStreamer installation."
msgstr "Satu atau lebih plugin telah hilang dalam penginstalan GStreamer-mu."

#: gstreamer/plugininstaller.cpp:188
msgctxt "Phonon::Gstreamer::PluginInstaller|"
msgid "Missing codec helper script assistant."
msgstr "Asisten penunjang skrip kodek hilang."

#: gstreamer/plugininstaller.cpp:190
msgctxt "Phonon::Gstreamer::PluginInstaller|"
msgid "Plugin codec installation failed."
msgstr "Penginstalan kodek plugin, gagal."

#: gstreamer/plugininstaller.cpp:214
msgctxt "Phonon::Gstreamer::PluginInstaller|"
msgid "Phonon attempted to install an invalid codec name."
msgstr "Phonon berupaya untuk menginstal sebuah nama kodek yang tak absah."

#: gstreamer/plugininstaller.cpp:217
msgctxt "Phonon::Gstreamer::PluginInstaller|"
msgid "The codec installer crashed."
msgstr "Penginstal kodek, mogok."

#: gstreamer/plugininstaller.cpp:220
msgctxt "Phonon::Gstreamer::PluginInstaller|"
msgid "The required codec could not be found for installation."
msgstr "Kodek yang dibutuhkan tidak ditemukan untuk penginstalan."

#: gstreamer/plugininstaller.cpp:223
msgctxt "Phonon::Gstreamer::PluginInstaller|"
msgid "An unspecified error occurred during codec installation."
msgstr "Sebuah error tak tertentu terjadi selagi penginstalan kodek."

#: gstreamer/plugininstaller.cpp:226
msgctxt "Phonon::Gstreamer::PluginInstaller|"
msgid "Not all codecs could be installed."
msgstr "Tidak semua kodek bisa diinstal."

#: gstreamer/plugininstaller.cpp:229
msgctxt "Phonon::Gstreamer::PluginInstaller|"
msgid "User aborted codec installation"
msgstr "Pengguna menggugurkan penginstalan kodek"

#: gstreamer/plugininstaller.cpp:240
msgctxt "Phonon::Gstreamer::PluginInstaller|"
msgid "Could not update plugin registry after update."
msgstr "Tidak bisa memperbarui registri plugin sesudah pembaruan."

